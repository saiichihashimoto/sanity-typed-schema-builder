{
  "checkers": [
    "typescript"
  ],
  "packageManager": "npm",
  "mutate": [
    "src/**/*.ts",
    "!**/*.{d,spec}.ts"
  ],
  "reporters": [
    "clear-text",
    "progress"
  ],
  "testRunner": "jest",
  "tsconfigFile": "tsconfig.json",
  "jest": {
    "configFile": "jest.config.js"
  },
  "thresholds": {
    "high": 100,
    "low": 100,
    "break": 100
  }
}
